moddata()
{
chmod 755 /data/media/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks
chmod 660 /data/media/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks/*
rm -rf /data/data/com.tencent.ig/lib/libcubehawk.so
rm -rf /data/data/com.tencent.ig/lib/libTDataMaster.so
rm -rf /data/data/com.tencent.ig/lib/libzip.so
rm -rf /data/data/com.tencent.ig/lib/libzlib.so
rm -rf /data/data/com.tencent.ig/lib/libtprt.so
rm -rf /data/data/com.tencent.ig/lib/libtersafe.so
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libtprt.so bs=36756 count=100 &> /dev/null
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libtersafe.so bs=36756 count=9067 &> /dev/null
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libcubehawk.so bs=2000 count=7075  &> /dev/null
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libTDataMaster.so bs=2100 count=9067  &> /dev/null
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libzip.so bs=2300 count=8977  &> /dev/null
dd if=/dev/urandom of=/data/data/com.tencent.ig/lib/libzlib.so bs=2400 count=7899  &> /dev/null
chmod 755 /data/data/com.tencent.ig/lib/*
rm -rf /data/data/com.tencent.ig/shared_prefs/*  &> /dev/null
chmod -R 555 /data/data/com.tencent.ig/shared_prefs
$BUSYBOX/chattr -i /data/data/com.tencent.ig/databases
chmod -R 555 /data/data/com.tencent.ig/databases
}

